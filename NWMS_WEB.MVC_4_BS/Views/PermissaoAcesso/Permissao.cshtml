                                                        <ul>
                                                            <li>
                                                                <input value="@submen02.CODMEN.ToString()@idItemMenu" type="checkbox"><span>@submen02.DESMEN</span>
                                                                @foreach (NUTRIPLAN_WEB.MVC_4_BS.Model.MenuModel submen03 in ListSubMen03)
