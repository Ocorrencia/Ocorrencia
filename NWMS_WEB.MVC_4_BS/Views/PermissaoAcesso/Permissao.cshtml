                                         }
                                     }
                                     else
                                     {
                                        <ul>
                                            <li>
                                                <input value="@submen01.CODMEN.ToString()@idItemMenu" type="checkbox"><span>@submen01.DESMEN</span>
                                                @foreach (NUTRIPLAN_WEB.MVC_4_BS.Model.MenuModel submen02 in ListSubMen02)
